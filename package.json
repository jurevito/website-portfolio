		"typescript": "^5.6.3",
		"vite": "^6.2.6",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@types/three": "^0.149.0",
		"gsap": "^3.11.5",
		"lucide-svelte": "^0.454.0",
		"three": "^0.149.0"
	}
