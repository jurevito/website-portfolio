		"@sveltejs/kit": "^2.7.3",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"bits-ui": "^1.0.0-next.65",
		"clsx": "^2.1.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"gh-pages": "^5.0.0",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
